{
    "id": "my-api",
    "name": "my-api",
    "gravitee": "4.0.0",
    "type": "proxy",
    "listeners": [
        {
            "type": "http",
            "paths": [
                {
                    "path": "/test"
                }
            ],
            "entrypoints": [
                {
                    "type": "http-proxy"
                }
            ]
        }
    ],
    "endpointGroups": [
        {
            "name": "default-group",
            "type": "http-proxy",
            "endpoints": [
                {
                    "name": "default",
                    "type": "http-proxy",
                    "weight": 1,
                    "inheritConfiguration": false,
                    "configuration": {
                        "target": "http://localhost:8080/team"
                    },
                    "sharedConfigurationOverride": {
                        "http": {
                            "connectTimeout": 3000,
                            "readTimeout": 60000
                        }
                    }
                }
            ]
        }
    ],
    "flows": [],
    "resources": [
        {
            "name": "dummy-oauth2-resource",
            "enabled": true,
            "type": "dummy-oauth2-resource",
            "configuration": {}
        }
    ]
}
